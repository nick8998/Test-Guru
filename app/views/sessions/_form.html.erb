<%= form_with url: sessions_path, local: true do |form| %>

 
  <%=flash_message(:alert)%>    


  <p>
    <%= form.label :email %><br>
    <%= form.text_field :email%>
  </p><br>

  <p>
    <%= form.label :password %><br>
    <%= form.password_field :password%>
  </p>

  <p><%= form.submit 'Enter'%></p>
<% end %>